{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/ux",
  "assets": [
    "./assets/**/*.*"
  ],
  "lib": {
    "styleIncludePaths": [
      "./assets/styles"
    ],
    "entryFile": "./public-api.ts",
    "umdModuleIds": {
      "ngx-bootstrap/dropdown": "ngx-bootstrap/dropdown",
      "@swimlane/ngx-datatable": "@swimlane/ngx-datatable",
      "@ngrx/store": "@ngrx/store",
      "@ngrx/effects": "@ngrx/effects",
      "ngx-bootstrap/collapse": "ngx-bootstrap/collapse",
      "ngx-bootstrap/tooltip": "ngx-bootstrap/tooltip",
      "cropperjs": "cropperjs"
    }
  },
  "whitelistedNonPeerDependencies": [
    "@swimlane/ngx-datatable",
    "cropperjs"
  ]
}
